<phpunit 
    bootstrap="tests/Bootstrap.php"
    backupGlobals="false"
         backupStaticAttributes="false"
         convertErrorsToExceptions="true"
         convertNoticesToExceptions="true"
         convertWarningsToExceptions="true"
         syntaxCheck="false"
         processIsolation="true"
         colors="true"
         verbose="true">
    <testsuites>
        <testsuite name="Menir">
            <directory>tests</directory>
                    
        </testsuite>
    </testsuites>
<logging>
<log type="coverage-html" target="./tests/reports" charset="UTF-8" highlight="false" lowUpperBound="35" highLowerBound="70"/>
</logging>
</phpunit>